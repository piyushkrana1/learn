{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch API in Debug Mode",
      "program": "${workspaceFolder}/index.js",
      "console": "integratedTerminal",
      "runtimeExecutable": "node",
      "env": {
        "NODE_ENV": "development"
      },
      "restart": true,
      "autoAttachChildProcesses": true,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
